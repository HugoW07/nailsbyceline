{
  "name": "hugow-cv-website",
  "version": "1.0.0",
  "description": "Personal CV website for Hugo using Bun",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "build": "bun build src/app.ts --outdir dist --target bun",
    "start": "bun run src/app.ts",
    "dev": "bun --watch src/app.ts",
    "prod": "bun run dist/app.js"
  },
  "keywords": [
    "cv",
    "portfolio",
    "bun",
    "typescript"
  ],
  "author": "Hugo",
  "license": "MIT",
  "dependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.5.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
